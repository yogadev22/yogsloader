extern "C" JNIEXPORT void JNICALL
Java_com_starkskillz_pubg_FloatLogo_SettingValue(JNIEnv *, jobject, jint code, jboolean jboolean1) {
	switch ((int)code) {
	case 2:
		isPlayerTeamID = jboolean1;
