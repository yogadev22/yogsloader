		break;
	case 15:
		options.isRadar = jboolean1;
		break;
	case 16:
		isPlayerBoxx = jboolean1;
		break;
	case 17:
		isNoBot = jboolean1;
		break;
	case 18:
		options.InputInversion = jboolean1;
		break;
	}
}

